<h1>SQUAD</h1>
    <table class="table-bordered">
    <th>Squad Name</th>
    <th>Aim</th>
    <th>Hero name</th>
    #foreach ($squad in $squads)

        <tr>
        <td>$squad.getDescription()</td>
         <td>$squad.getStrength()</td>
          <td>$squad.getheroname()<td>
        </tr>
     #end
</table>
